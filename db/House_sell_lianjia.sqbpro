ql><sql name="SQL 1">CREATE TABLE compound_buildyear AS
SELECT 小区, GROUP_CONCAT(house_build_year, ',') AS built_years, COUNT(house_build_year) as number_of_build_years
FROM (
    SELECT DISTINCT 小区, house_build_year
    FROM sh_home_selldata_2ndhand_final
)
GROUP BY 小区;
